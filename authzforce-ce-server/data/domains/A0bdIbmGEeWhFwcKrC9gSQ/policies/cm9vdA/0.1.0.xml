<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<PolicySet xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="urn:oasis:names:tc:xacml:3.0:core:schema:wd-17 http://docs.oasis-open.org/xacml/3.0/xacml-core-v3-schema-wd-17.xsd"
  xmlns="urn:oasis:names:tc:xacml:3.0:core:schema:wd-17"
  PolicySetId="root"
  Version="0.1.0"
  PolicyCombiningAlgId="urn:oasis:names:tc:xacml:3.0:policy-combining-algorithm:deny-unless-permit">
  <Target />
  <Policy RuleCombiningAlgId="urn:oasis:names:tc:xacml:3.0:rule-combining-algorithm:deny-unless-permit" PolicyId="permit-all" Version="0.1.0">
    <Target />
    <Rule Effect="Permit" RuleId="permit-all">
      <Target>
        <AnyOf>
          <AllOf>
            <Match MatchId="urn:ogc:def:function:geoxacml:1.0:geometry-equals">
              <AttributeValue DataType="urn:ogc:def:dataType:geoxacml:1.0:geometry">POINT (11.57549 48.13743)</AttributeValue>
              <AttributeDesignator AttributeId="urn:oasis:names:tc:xacml:1.0:subject:subject-location" Category="urn:oasis:names:tc:xacml:3.0:attribute-category:environment" DataType="urn:ogc:def:dataType:geoxacml:1.0:geometry" MustBePresent="true"/>
            </Match>
          </AllOf>
        </AnyOf>
        <AnyOf>
          <AllOf>
            <Match MatchId="urn:ogc:def:function:geoxacml:1.0:geometry-contains">
              <AttributeValue DataType="urn:ogc:def:dataType:geoxacml:1.0:geometry">POLYGON ((0 0, 0 90, 90 90, 90 0, 0 0))</AttributeValue>
              <AttributeDesignator AttributeId="urn:oasis:names:tc:xacml:1.0:subject:subject-location" Category="urn:oasis:names:tc:xacml:3.0:attribute-category:environment" DataType="urn:ogc:def:dataType:geoxacml:1.0:geometry" MustBePresent="true"/>
            </Match>
          </AllOf>
        </AnyOf>
      </Target>
    </Rule>
  </Policy>
</PolicySet>
